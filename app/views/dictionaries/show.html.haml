%p#notice= notice
%p
  %strong Name:
  = @dictionary.name
%p
  %strong Definitions:
  %br/
  %table
    %thead
      %tr
        %th Dictionary
        %th Initialism
        %th Meaning
        %th{:colspan => "3"}
    %tbody
      - @dictionary.definitions.sort.each do |definition|
        = render definition
      =# @dictionary.definitions.collect { |d| "#{d.initialism}: #{d.meaning}" }.join("<br\>\n").html_safe
= link_to 'Edit', edit_dictionary_path(@dictionary)
|
= link_to 'Back', dictionaries_path
